{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/"
      ]
    },
    "lint": {
      "cache": false
    },
    "lint:fix": {
      "cache": false
    },
    "type-check": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "clean": {
      "cache": false,
      "outputs": [
        "dist/"
      ]
    }
  }
}